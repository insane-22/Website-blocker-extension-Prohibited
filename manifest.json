{
    "manifest_version": 3,
    "name": "Prohibited",
    "version": "0.1.1",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage"
    ],
    "icons":{
      "128":"images/logo.png"
    }
  }